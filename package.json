{
  "name": "react-sdk-components",
  "version": "0.22.0",
  "description": "React SDK Infrastructure: bridge and components",
  "main": "index.ts",
  "scripts": {
    "build-sdk-complete": "run-p -l lint build-sdk",
    "prebuild-sdk": "npm run delete-tsbuildinfo && npm run clear-lib && npm run prepare-code-for-compilation",
    "build-sdk": "npm run compile-ts",
    "postbuild-sdk": "npm run copy-static-files-to-lib",
    "pack-sdk": "npm pack ./packages/react-sdk-components",
    "ci": "npm run clean && npm install",
    "clean": "node rimraf-standalone.js ./node_modules",
    "delete-tsbuildinfo": "rm -f packages/react-sdk-components/*.*.tsbuildinfo",
    "clear-lib": "rm -rf packages/react-sdk-components/lib",
    "prepare-code-for-compilation": "node scripts/build-exports.js",
    "compile-ts": "tsc -b tsconfig.build.json",
    "copy-static-files-to-lib": "node scripts/copy-static-to-lib.js",
    "lint": "npx eslint \"**/src/**\"",
    "lint-errors-only": "npx eslint \"packages/react-sdk-components/src/**\" --quiet",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Pegasystems",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@date-io/dayjs": "^1.3.13",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/pickers": "^3.3.10",
    "@types/react": "^16.13.1",
    "@unicef/material-ui-currency-textfield": "^0.8.6",
    "dayjs": "^1.10.6",
    "downloadjs": "^1.4.7",
    "fast-deep-equal": "^3.1.3",
    "lodash.isequal": "^4.5.0",
    "material-ui-phone-number": "^2.2.6",
    "react": "^16.13.1",
    "react-datepicker": "^4.8.0",
    "throttle-debounce": "^5.0.0"
  },
  "devDependencies": {
    "@pega/commitlint-config": "^0.4.0",
    "@pega/commitlint-plugin": "^0.4.0",
    "@pega/configs": "^0.4.0",
    "@pega/cspell-config": "^0.4.0",
    "@pega/eslint-config": "^0.4.0",
    "@pega/prettier-config": "^0.4.0",
    "@pega/stylelint-config": "^0.4.0",
    "@pega/tsconfig": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "commitlint": "^17.3.0",
    "cspell": "^6.17.0",
    "eslint": "^8.30.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.17.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "prop-types": "^15.7.2",
    "rimraf-standalone": "^2.6.1-7",
    "stylelint": "^14.16.0",
    "typescript": "^4.7.4",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.7.4"
  }
}
