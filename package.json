{
  "name": "react-sdk-components",
  "version": "0.22.0",
  "description": "React SDK Infrastructure: bridge and components",
  "main": "index.ts",
  "scripts": {
    "prebuild-sdk": "npm run delete-tsbuildinfo && npm run clear-lib && npm run prepare-code-for-compilation",
    "build-sdk": "npm run compile-ts",
    "postbuild-sdk": "npm run copy-static-files-to-lib",
    "ci": "npm run clean && npm install",
    "clean": "node rimraf-standalone.js ./node_modules",
    "delete-tsbuildinfo": "rm -f packages/react-sdk-components/*.*.tsbuildinfo",
    "clear-lib": "rm -rf packages/react-sdk-components/lib",
    "prepare-code-for-compilation": "node scripts/build-exports.js",
    "compile-ts": "tsc -b tsconfig.build.json",
    "copy-static-files-to-lib": "node scripts/copy-static-to-lib.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Pegasystems",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@date-io/dayjs": "^1.3.13",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/pickers": "^3.3.10",
    "@types/react": "^16.13.1",
    "@unicef/material-ui-currency-textfield": "^0.8.6",
    "dayjs": "^1.10.6",
    "downloadjs": "^1.4.7",
    "fast-deep-equal": "^3.1.3",
    "lodash.isequal": "^4.5.0",
    "material-ui-phone-number": "^2.2.6",
    "react": "^16.13.1",
    "react-datepicker": "^4.8.0",
    "throttle-debounce": "^5.0.0"
  },
  "devDependencies": {
    "@pega/tsconfig": "0.3.4",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "rimraf-standalone": "^2.6.1-7",
    "typescript": "~4.7.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.7.4"
  }
}
